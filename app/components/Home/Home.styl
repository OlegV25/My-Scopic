.Home
  width 100%
  height 100%
  border-radius 5px
  padding 50px

  .Home__input
    width 100%
    border 1px solid black
    border-radius 5px
    padding 5px 10px
    margin-bottom 10px

  .Home__button
    width 400px
    height 50px
    border 1px solid black
    border-radius 5px
    text-align center
    line-height 50px
    margin auto
    font-size 20px
    cursor pointer
    margin-bottom 10px

  .Home__node-tree
    width 100%
    height 300px
    overflow auto
    border 1px solid black
    border-radius 5px
    margin-bottom 10px
    padding 5px

    ul
      list-style-type none

    img
      width 20px
      height 10px
